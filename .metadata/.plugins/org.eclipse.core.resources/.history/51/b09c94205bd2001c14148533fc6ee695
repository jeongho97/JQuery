<input type="button" id="btn" value="버튼">

<script type="text/javascript">

$(document).ready(function(){
	
	$("#btn").click(function(){
		
		//getter
		let value=$("#text").val();
		//alert(value);
		//setter
		$("#text").val("hello World");
	})
	
})

</script>

